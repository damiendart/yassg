{
  "authors": [
    {
      "email": "damiendart@pobox.com",
      "homepage": "https://www.robotinaponcho.net",
      "name": "Damien Dart",
      "role": "Developer"
    }
  ],
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Yassg\\": "src/"
    }
  },
  "autoload-dev": {
    "files": [
      "tests/helpers.php"
    ],
    "psr-4": {
      "Yassg\\Tests\\": "tests/"
    }
  },
  "bin": [
    "bin/yassg"
  ],
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": false
    }
  },
  "description": "Yet another static site generator.",
  "license": "MIT",
  "name": "damiendart/yassg",
  "prefer-stable": true,
  "require": {
    "php": "^8.2",
    "league/commonmark": "^2.4.1",
    "php-di/php-di": "^7.0.5",
    "psr/event-dispatcher": "^1.0",
    "symfony/filesystem": "^6.3.1",
    "symfony/finder": "^6.3.3",
    "symfony/yaml": "^6.3.3",
    "twig/twig": "^3.7.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.26.1",
    "phpstan/phpstan": "^1.10.34",
    "phpunit/phpunit": "^10.3.4",
    "vimeo/psalm": "^5.15.0"
  }
}
