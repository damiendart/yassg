{
  "authors": [
    {
      "email": "damiendart@pobox.com",
      "homepage": "https://www.robotinaponcho.net",
      "name": "Damien Dart",
      "role": "Developer"
    }
  ],
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Yassg\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Yassg\\Tests\\": "tests/"
    }
  },
  "bin": [
    "bin/yassg"
  ],
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": false
    }
  },
  "description": "Yet another static site generator.",
  "license": "MIT",
  "name": "damiendart/yassg",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "vcs",
      "url": "https://www.robotinaponcho.net/git/toolbox-sass.git"
    }
  ],
  "require": {
    "php": "^8.0",
    "league/commonmark": "^2.3",
    "php-di/php-di": "^6.4",
    "psr/event-dispatcher": "^1.0",
    "symfony/filesystem": "^6.0.7",
    "symfony/finder": "^6.0.3",
    "symfony/yaml": "^6.0.3",
    "twig/twig": "^3.3.10"
  },
  "require-dev": {
    "damiendart/toolbox-sass": "dev-master",
    "friendsofphp/php-cs-fixer": "^3.8",
    "phpunit/phpunit": "^9.5.20",
    "vimeo/psalm": "^4.22"
  }
}
